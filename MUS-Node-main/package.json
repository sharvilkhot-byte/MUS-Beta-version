{
    "name": "mus-node-root",
    "private": true,
    "main": "api/index.ts",
    "scripts": {
        "build": "cd client && npm install --include=dev && npm run build",
        "postinstall": "cd client && npm install --include=dev",
        "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
        "dev:server": "cd server && npm run dev",
        "dev:client": "cd client && npm run dev"
    },
    "dependencies": {
        "@axe-core/puppeteer": "^4.11.0",
        "@google/genai": "^1.34.0",
        "@sparticuz/chromium": "^119.0.0",
        "@supabase/supabase-js": "^2.39.0",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "jsonrepair": "^3.13.1",
        "puppeteer-core": "^21.5.2"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/node": "^20.10.0",
        "concurrently": "^8.2.2",
        "puppeteer": "^21.5.2",
        "ts-node": "^10.9.1",
        "typescript": "^5.3.3"
    },
    "engines": {
        "node": ">=18.0.0"
    }
}